<%= simple_form_for(@cartum) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :descripcion %>
    <%= f.input :fecha %>
    <%= f.input :estado %>
    <%= f.input :precio %>
    <%= f.association :local %>
    <%= f.association :tipomenu %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
