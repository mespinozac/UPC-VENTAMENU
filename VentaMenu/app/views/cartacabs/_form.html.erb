<%= simple_form_for @cartacab, :html => { :class => 'form-horizontal' } do |f| %>
  <%= f.input :descripcion %>
  <%= error_span(@cartacab[:descripcion]) %>
  <%= f.input :fecha %>
  <%= error_span(@cartacab[:fecha]) %>
  <%= f.input :estado %>
  <%= error_span(@cartacab[:estado]) %>
  <%= f.input :precio %>
  <%= error_span(@cartacab[:precio]) %>
  <%= f.input :local_id %>
  <%= error_span(@cartacab[:local_id]) %>
  

  <%= f.button :submit, :class => 'btn-primary' %>
  <%= link_to t('.cancel', :default => t("helpers.links.cancel")),
                cartacabs_path, :class => 'btn btn-default' %>
<% end %>
