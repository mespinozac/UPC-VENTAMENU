<%- model_class = Cartadetalle -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human.pluralize.titleize %></h1>
</div>
<script>
  var tipoMenu="";
</script>
<% @tipomenus.each do |tipo| %>
<div><h1><%= tipo.nombre %></h1></div>
<table class="table table-striped">
  <thead>
    <tr>
      <th><%= model_class.human_attribute_name(:id) %></th>
      <th><%= model_class.human_attribute_name(:estado) %></th>
      <th><%= model_class.human_attribute_name(:cartacab_id) %></th>
      <th><%= model_class.human_attribute_name(:menu_id) %></th>
      <th><%= model_class.human_attribute_name(:tipomenu_id) %></th>
      <th><%= model_class.human_attribute_name(:created_at) %></th>
      <th><%=t '.actions', :default => t("helpers.actions") %></th>
    </tr>
  </thead>
  <tbody>
    <% tipo.cartadetalle.each do |cartadetalle| %>
    
    <script>
      alert('richard')
    </script>
      <tr>
        <td><%= link_to cartadetalle.id, cartadetalle_path(cartadetalle) %></td>
        <td><%= cartadetalle.estado %></td>
        <td><%= cartadetalle.cartacab_id %></td>
        <td><%= cartadetalle.menu.nombre %></td>
        <td><%= cartadetalle.tipomenu.nombre %></td>
        <td><%=l cartadetalle.created_at %></td>
        <td>
          <%= link_to t('.edit', :default => t("helpers.links.edit")),
                      edit_cartadetalle_path(cartadetalle), :class => 'btn btn-default btn-xs' %>
          <%= link_to t('.destroy', :default => t("helpers.links.destroy")),
                      cartadetalle_path(cartadetalle),
                      :method => :delete,
                      :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
                      :class => 'btn btn-xs btn-danger' %>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>
<% end %>

<%= link_to t('.new', :default => t("helpers.links.new")),
            new_cartadetalle_path,
            :class => 'btn btn-primary' %>
