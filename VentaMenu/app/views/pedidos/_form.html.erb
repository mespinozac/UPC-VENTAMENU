<%= simple_form_for(@pedido) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :fecha %>
    <%= f.input :horapedido %>
    <%= f.input :horaentrega %>
    <%= f.input :estado %>
    <%= f.input :montopedido %>
    <%= f.association :cartacab %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
